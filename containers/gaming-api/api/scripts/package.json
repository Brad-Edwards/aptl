{
  "name": "gaming-api-data-generation",
  "version": "1.0.0",
  "description": "Data generation scripts for Gaming API CTF scenario",
  "main": "generate-data.js",
  "scripts": {
    "generate": "node generate-data.js",
    "generate:verbose": "node generate-data.js --verbose",
    "generate:dry": "node generate-data.js --dry",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/tests/**",
      "!**/coverage/**",
      "!**/reports/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [],
    "testTimeout": 30000
  },
  "keywords": [
    "gaming",
    "api",
    "data-generation",
    "ctf",
    "cybersecurity",
    "testing"
  ],
  "author": "APTL Gaming API Data Generator",
  "license": "MIT"
}