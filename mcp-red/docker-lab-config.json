{
  "version": "1.0.0",
  "server": {
    "name": "kali-red-team",
    "version": "1.0.0",
    "description": "APTL Kali Red Team MCP Server - Provides AI agents with secure access to Kali Linux red team operations in the APTL (Advanced Purple Team Lab) environment.",
    "toolPrefix": "kali",
    "targetName": "Kali Linux",
    "configKey": "kali",
    "envPrefix": "APTL_KALI"
  },
  "lab": {
    "name": "aptl-local",
    "network_subnet": "172.20.0.0/16"
  },
  "containers": {
    "kali": {
      "container_name": "aptl-kali",
      "container_ip": "localhost", 
      "ssh_key": "~/.ssh/aptl_lab_key",
      "ssh_user": "kali",
      "ssh_port": 2023,
      "enabled": true
    }
  },
  "mcp": {
    "server_name": "aptl-kali-red-team-mcp",
    "allowed_networks": ["172.20.0.0/16", "127.0.0.0/8"],
    "max_session_time": 3600,
    "audit_enabled": true,
    "log_level": "info"
  }
}